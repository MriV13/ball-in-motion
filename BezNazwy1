#include <allegro.h>
#include <fstream>



int main()
{
	allegro_init();
	install_keyboard();
	set_color_depth( 32 );
    set_gfx_mode( GFX_AUTODETECT_WINDOWED, 640, 480, 0, 0 );
    clear_to_color( screen, makecol( 0, 255, 255 ) );
    
    
    
    
    allegro_exit();

return 0;
}
END_OF_MAIN();




















































































